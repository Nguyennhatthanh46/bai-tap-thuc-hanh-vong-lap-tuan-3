#include<iostream>
#include<cmath>

using namespace std;
int main (){ 
int x;
int n;
double y = 0,z = 0;
double t = 1, t1 =1;
cin>>n>>x;
for(int i =0;i<=n;i++){
	y+=pow(x,i);
	z+=pow(x,2*i);
}
for(int i = 1; i<=n;i++){
	t1*=i;
	t+=(pow(x,i))/(t1);
}
cout<<y<<endl;
cout<<z<<endl;
cout<<t<<endl;
	return 0;
}
